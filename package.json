{
  "name": "burger-queen-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "pretest": "eslint --fix .",
    "start": "nodemon ./functions/api.js",
    "test:e2e": "jest --verbose --testPathPattern e2e --globalSetup ./e2e/globalSetup --globalTeardown ./e2e/globalTeardown --runInBand",
    "test:unit": "jest --verbose --testPathIgnorePatterns e2e",
    "test": "npm run test:e2e && npm run test:unit",
    "lint": "eslint --fix .",
    "docs:generate": "jsdoc -c jsdoc.conf --readme README-docs.md",
    "docs:deploy": "rm -rf docs && npm run docs:generate && gh-pages -d docs"
  },
  "dependencies": {
    "@netlify/functions": "^2.6.0",
    "@shelf/jest-mongodb": "^4.1.7",
    "@types/express": "^4.17.21",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^8.12.2",
    "mongoose": "^8.2.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "docdash": "^1.2.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.22.1",
    "gh-pages": "^3.1.0",
    "jest": "^27.0.1",
    "jsdoc": "^3.6.6",
    "jsdoc-http-plugin": "^0.3.2",
    "node-fetch": "^3.1.0",
    "nodemon": "^3.1.0",
    "tree-kill": "^1.2.2"
  }
}
